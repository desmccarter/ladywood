<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="PageBean" class="com.dmcc.pagegen.page.mccarterp.McCarterDotNetPage">
    
         <property name="elementDefinitions">
            <list>
                <ref bean="InputBean" />
                <ref bean="ABean" />
            </list>
        </property>
        
    </bean>
    
    <bean id="InputBean" class="com.dmcc.pagegen.tags.generic.Input"/>
    <bean id="ABean" class="com.dmcc.pagegen.tags.generic.A"/>
</beans>